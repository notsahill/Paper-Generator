{
  "version": 2,
  "builds": [
    { "src": "/server.js", "use": "@vercel/node" },
    { "src": "/controllers/**", "use": "@vercel/node" },
    { "src": "/models/**", "use": "@vercel/node" },
    { "src": "/configuration/**", "use": "@vercel/node" }
  ] ,
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ]
}
